generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

//datasource db {
//  provider = "mysql"
//  url      = env("DATABASE_URL")
//}

model dBProperties {
  database_id String @id
  result  Bytes?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model dBRows {
  database_id String @id
  query   String?
  result  Bytes?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model pageProperties {
  page_id String @id
  result  Bytes?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model blockChildren {
  block_id String @id
  result  Bytes?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}